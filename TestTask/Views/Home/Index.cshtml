@{
    ViewBag.Title = "Home Page";
}

@foreach (var user in ViewBag.Users)
{
    <div>@user.Name</div>
}
<div id="app">
    {{ message }}
</div>
@Scripts.Render("~/node_modules/vue/dist/vue.min.js")

<script>
    const v = new Vue({
        el: '#app',
        data: {
            message: 'Hello Vue.js!'
        }
    })
</script>